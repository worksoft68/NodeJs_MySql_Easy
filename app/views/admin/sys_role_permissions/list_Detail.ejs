	function saveInsertsys_role_permissions() {
		let data=getDataFormsys_role_permissions();
		if(data==false) {
			$('#btnSave').prop('disabled',false);
			return false;
		}
		var link=$('#linkPrefix').val()+'savesys_role_permissions';
		$.ajax({
			type: 'post',
			url: link,
			data: data,
			dataType: "json",
			success: function(response) {
				var success=response.success;
				var result=response.data;
				if((success=='false')||(success==false))
					showNoticeDanger(language.Message_AddNewError+' ('+response.message+')');
				else if((success!=true)&&(success!='true'))
					showNoticeDanger(result.message);
				else {
					insertRowsys_role_permissions(result.data);
					clearFormsys_role_permissions();
				}
			},
			failure: function(response) {
				showErrorMessageSave(response,language.Message_AddNewError);
			},
			error: function(response) {
				showErrorMessageSave(response,language.Message_AddNewError);
			},
		});
		$('#btnSave').prop('disabled',false);
	}

