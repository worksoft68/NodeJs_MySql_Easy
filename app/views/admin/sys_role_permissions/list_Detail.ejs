			showNoticeDanger(language.Message_HaveNotSelectedItemUpdate);
			return false;
		}
		var link=$('#linkPrefix').val()+'updatesys_role_permissions';
		$.ajax({
			type: 'put',
			url: link,
			data: data,
			dataType: "json",
