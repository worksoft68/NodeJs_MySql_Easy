				function saveUpdateSys_Role_Permission() {
					let data=getDataFormSys_Role_Permission();
					if(data==false) {
						$('#btnUpdateSys_Role_Permission').prop('disabled',false);
						return false;
					}
					if(data.role_permissions_iddHidden=="") {
						$('#btnUpdateSys_Role_Permission').prop('disabled',false);
						showNoticeDanger(language.Message_HaveNotSelectedItemUpdate);
						return false;
					}
					var link=$('#linkPrefix').val()+'updateRolePermission';
					$.ajax({
						type: 'put',
						url: link,
						data: data,
						dataType: "json",
						success: function(response) {
							var success=response.success;
							var result=response.data;
							if((success=='false')||(success==false)) {
								let FunctionAlreadyExists="";
								if(response.message=="FunctionAlreadyExists") {
									FunctionAlreadyExists=language.Message_FunctionAlreadyExists;
								}
								showNoticeDanger(language.Message_UpdateError+". "+FunctionAlreadyExists);
								return false;
							}
							else if((success!=true)&&(success!='true')) {
								showNoticeDanger(result.message);
								return false;
							}
							else {
								updateRowSys_Role_Permission(result.data);
								clearFormSys_Role_Permission();
							}
						},
						failure: function(response) {
							showErrorMessageSave(response,language.Message_UpdateError);
						},
						error: function(response) {
							showErrorMessageSave(response,language.Message_UpdateError);
						}
					});
					$('#btnUpdateSys_Role_Permission').prop('disabled',false);
				}

